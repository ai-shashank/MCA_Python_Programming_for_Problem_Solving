   "source": [
    "# A program to reverse a number\n",
    "num = int(input(\"Enter a number: \"))\n",
    "print(\"Enter a number:\", num) # show what the user entered\n",
    "\n",
    "rev_num = 0\n",
    "while num > 0:\n",
    "    digit = num % 10\n",
    "    rev_num = rev_num * 10 + digit\n",
    "    num = num // 10\n",
    "\n",
    "print(\"Reversed Number:\", rev_num)"
   ]
  }
