   "source": [
    "# Writing a program that removes duplicates and displays the sorted list\n",
    "\n",
    "a = [\"apple\", \"banana\", \"apple\", \"orange\", \"banana\"]\n",
    "print(\"Original list:\", a)\n",
    "\n",
    "unique_list = list(set(a))\n",
    "unique_list.sort()\n",
    "\n",
    "print(\"Sorted list without duplicates:\", unique_list)"
   ]
  }
